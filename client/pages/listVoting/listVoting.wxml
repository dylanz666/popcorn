<view class="container">

  <view class="voteItems" wx:for="{{voteItems}}">
    <navigator url="../voting/voting?id={{item.id}}&name={{item.name}}&items={{item.items}}" open-type="navigate" class="voteItems-text">
      <text class="voteItems-text">{{item.index}}.{{item.name}}</text>
    </navigator>
  </view>

  <navigator url="../addVote/addVote" open-type="navigate" class="voteItems-text">
    <button type="primary" class="createVote">创建新投票</button>
  </navigator>

</view>